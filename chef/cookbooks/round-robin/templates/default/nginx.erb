events {
  worker_connections 768;
  # multi_accept on;
}

http {
  upstream roundrobin {
    server <%= node['round-robin']['app_server_1'] %>:8484;
    server <%= node['round-robin']['app_server_2'] %>:8484;
  }

  server {
    listen 80;
    location / {
      proxy_pass http://roundrobin;
    }
  }
}